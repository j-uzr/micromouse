  void turnRight() {

    delay(1000);
    analogWrite(5, 0); // FWD Right Off
    analogWrite(6, 0); // REV Left Off
    analogWrite(7, 100); // REV Right On
    analogWrite(4, 150); // FWD Left On
    delay(5000);

  }
  
  void turnLeft() {

    delay(1000);
    analogWrite(4, 0); // FWD Left Off
    analogWrite(7, 0); // REV Right Off
    analogWrite(6, 100); // REV Left On
    analogWrite(5, 150); // FWD Right On
    delay(5000);

  }
